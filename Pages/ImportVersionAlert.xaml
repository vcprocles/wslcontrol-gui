<Window x:Class="wslcontrol_gui.Pages.ImportVersionAlert"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wslcontrol_gui.Pages"
        mc:Ignorable="d"
        Title="ImportVersionAlert" Height="266" Width="343">
    <Grid>
        <Label Height="30" Margin="10,109,10,45">What WSL version you want to use for this distro?</Label>
        <Button Height="20" Width="40" Margin="17,144,243,20" Name="WSL1Button" Click="WSL1Button_Click">WSL1</Button>
        <Button Height="20" Width="40" Margin="81,144,179,20" Name="WSL2Button" Click="WSL2Button_Click">WSL2</Button>
        <Label Content="Specify the name for the new distro" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" Text="ImportedDistro" VerticalAlignment="Top" Width="120" Name="DistroNameBox"/>
    </Grid>
</Window>
