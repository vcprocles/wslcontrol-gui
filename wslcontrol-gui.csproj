<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows7.0</TargetFramework>
    <RootNamespace>wslcontrol_gui</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
	<Version>1.2.0.1</Version>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
    <StartupObject></StartupObject>
    <Platforms>AnyCPU</Platforms>
    <DebugType>embedded</DebugType>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <ApplicationIcon>Assets\wslcguiicon.ico</ApplicationIcon>
    <Company>vcprocles</Company>
  </PropertyGroup>
	
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>
	
  <ItemGroup>
    <Compile Remove="publish\**" />
    <EmbeddedResource Remove="publish\**" />
    <None Remove="publish\**" />
    <Page Remove="publish\**" />
  </ItemGroup>
	
  <ItemGroup>
    <Compile Remove="Pages\PerDistroPrefs.xaml.cs" />
  </ItemGroup>
	
  <ItemGroup>
    <Page Remove="Pages\PerDistroPrefs.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ModernWpfUI" Version="0.9.6" />
    <PackageReference Include="PeanutButter.INI" Version="3.0.58" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Assets\wslcguiicon.ico" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Pages\PerDistroPrefs.xaml" />
    <None Include="Pages\PerDistroPrefs.xaml.cs" />
  </ItemGroup>

</Project>
