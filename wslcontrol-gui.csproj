<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>wslcontrol_gui</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
	<UseWindowsForms>True</UseWindowsForms>
	<Version>1.4.0.1</Version>
    <StartupObject></StartupObject>
    <Platforms>AnyCPU</Platforms>
    <DebugType>embedded</DebugType>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <ApplicationIcon>Assets\wslcguiicon.ico</ApplicationIcon>
    <Company>vcprocles</Company>
  </PropertyGroup>
	
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>
	
  <ItemGroup>
    <Compile Remove="publish\**" />
    <EmbeddedResource Remove="publish\**" />
    <None Remove="publish\**" />
    <Page Remove="publish\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="PeanutButter.INI" Version="3.0.79" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Assets\wslcguiicon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Assets\Thirdparty\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="linux_companion\companion.pl">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
